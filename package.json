    "date-fns": "3.6.0",
    "framer-motion": "11.11.9",
    "geist": "1.3.1",
    "i18next": "23.4.4",
    "next": "15.0.1",
    "next-auth": "5.0.0-beta.25",
    "next-intl": "3.23.5",
    "next-safe-action": "7.9.2",
    "nuqs": "2.0.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-email": "3.0.1",
    "react-hook-form": "7.53.1",
    "react-i18next": "13.1.1",
    "resend": "4.0.0",
