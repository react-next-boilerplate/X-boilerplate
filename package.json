    "@vitejs/plugin-react": "4.3.1",
    "@vitest/coverage-v8": "2.0.5",
    "@vitest/ui": "2.0.5",
    "eslint": "8.56.0",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "husky": "8.0.3",
    "jsdom": "22.1.0",
    "lint-staged": "13.2.3",
    "prettier": "3.3.3",
    "typescript": "5.5.4",
    "vite-tsconfig-paths": "5.0.1",
    "vitest": "2.0.5"
